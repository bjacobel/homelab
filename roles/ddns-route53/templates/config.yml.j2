credentials:
  accessKeyID: "{{ ddns_access_key_id }}"
  secretAccessKey: "{{ ddns_secret_access_key }}"

route53:
  hostedZoneID: "Z3EWHA33A6DPWQ"
  recordsSet:
    - name: "lab.{{ internet_domain }}."
      type: "A"
      ttl: 300
    - name: "auth.{{ internet_domain }}."
      type: "A"
      ttl: 300
{% for service in ddns_services %}
    - name: "{{ service.name }}.{{ internet_domain }}."
      type: "A"
      ttl: 300
{% endfor %}
